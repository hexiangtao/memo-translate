server:
  port: 8080

spring:
  ai:
    openai:
      # 请填入你的 API Key
      api-key: sk-proj-YOUR_API_KEY_HERE
      # 兼容 OpenAI 的接口地址
      # 官方: https://api.openai.com
      # DeepSeek: https://api.deepseek.com
      # 其他中转: https://your-proxy.com
      base-url: https://api.openai.com
      chat:
        options:
          # 模型名称: gpt-3.5-turbo, gpt-4, deepseek-chat 等
          model: gpt-3.5-turbo
          temperature: 0.7
